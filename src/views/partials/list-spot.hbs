<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Name</th>
      <th>Categorey</th>
      <th>Description</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each swimlist.spots}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{categorey}}
        </td>
        <td>
          {{description}}
        </td>
        <td>
          <a href="/swimlist/{{../swimlist._id}}/deletespot/{{_id}}" class="button is-rounded">
            <span style="color: grey;">
            <i class="fa-regular fa-trash-can"></i>
            </span>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>